'' Why is this necessary?
'' http://en.wikipedia.org/wiki/Flat_memory_model



CON
    _clkmode        = xtal1 + pll16x           ' Feedback and PLL multiplier
    _xinfreq        = 5_000_000                ' External oscillator = 5 MHz


    ' screensize constants
    SCREEN_W = 128
    SCREEN_H = 64
    BITSPERPIXEL = 2
    
    PLAYER_W = 16
    PLAYER_H = 24

    
    SPEED = 2
    
    SONGS = 1
    SONGOFF = 255
    BAROFF = 254
    SNOP = 253
    SOFF = 252
    
    BARRESOLUTION = 8
    MAXBARS = 18    


OBJ
        lcd     :               "LameLCD" 
        gfx     :               "LameGFX"  
        ctrl    :               "LameControl"
        base    :               "LameFunctions"

VAR

    word    buffer[1024]
    
    
    long    pos_x
    long    pos_y
    long    pos_oldx
    long    pos_oldy

    byte    pos_dir
    long    pos_speed
    long    pos_speedx
    byte    pos_frame
    word    pos_count
    
    long    bulletbox_x
    long    bulletbox_y
    long    bulletbox
    long    bulletbox_dir
    long    bulletbox_speed
    byte    jumping
    
    
    long    x    
    long    y    
    long    tile
    long    tilecnt
    long    tilecnttemp
    
    long    xoffset
    long    yoffset


PUB Main

    dira~
        
    dira[24]~~
        
    gfx.Start(@buffer, lcd.Start) 

    gfx.LoadMap(@gfx_tiles_2b_tuxor, @map_supersidescroll)
    gfx.LoadFont(@gfx_chars_cropped, " ", 8, 8)
    
    ctrl.Start
    
    pos_x := 5
    pos_y := 0
    pos_dir := 1
    
    
    TitleScreen
    GameLoop
    
    

PUB TitleScreen | choice, clicked

    choice := 1
    clicked := 0
    repeat until not choice
        ctrl.Update
        gfx.DrawScreen

        gfx.Blit(@gfx_title)
        
        gfx.PutString(string("PRESS START"), 20, 45)

        if ctrl.A or ctrl.B
              if not clicked
                choice := 0
                clicked := 1
        else
              clicked := 0

PUB GameLoop
    repeat
    {{
            outa[24]~
        
            repeat x from 0 to 1000
        
            outa[24]~~
            
            repeat x from 0 to 1000
      }}      
            ctrl.Update
            
            gfx.ClearScreen
            'gfx.DrawMap(0, 0, 0,0, 16, 8)
            

            'if pos_dir == 3
            '    gfx.Sprite(@gfx_player,pos_x,pos_y, 3+pos_frame//3)
            'if pos_dir == 1
           ' gfx.Sprite(@gfx_vortex,pos_x,pos_y, 0)
            
'            gfx.Sprite(@gfx_player,8,0, 0)
 '           gfx.Sprite(@gfx_player,60,30, 0)
      
                
                
            pos_oldx := pos_x
            pos_oldy := pos_y    
                    
            if ctrl.Left or ctrl.Right
                if ctrl.Right
                    pos_x += SPEED
                    pos_dir := 1
    
                if ctrl.Left
                    pos_x -= SPEED
                    pos_dir := 3

                pos_count++
                if pos_count => 3
                    if pos_frame
                        pos_frame := 0
                    else
                        pos_frame := 1
                    pos_count := 0

                    
            else
                pos_frame := 0

            if gfx.TestMapCollision(pos_x, pos_y, PLAYER_W, PLAYER_H)
                pos_x := pos_oldx


            if ctrl.A
                if not jumping
                    'pos_dir := 0                   
                    pos_speed := -10
                    jumping := 1
            
            pos_speed += 1
            pos_y += pos_speed

            if gfx.TestMapCollision(pos_x, pos_y, PLAYER_W, PLAYER_H)
                if  pos_speed > 0
                    jumping := 0
                pos_y := pos_oldy
                pos_speed := 0
            
            if pos_speed > 0
                jumping := 1           
             
            ControlOffset
            gfx.DrawMap(xoffset, yoffset, 0,0, 16, 8)

            ' draw player
            if pos_dir == 3
                gfx.Sprite(@gfx_player,pos_x-xoffset,pos_y-yoffset, 3+pos_frame)
            if pos_dir == 1 
                gfx.Sprite(@gfx_player,pos_x-xoffset,pos_y-yoffset, pos_frame)


            if ctrl.B
                bulletbox := 1
                bulletbox_dir := pos_dir
                if pos_dir == 3
                    bulletbox_x := pos_x
                if pos_dir == 1
                    bulletbox_x := pos_x + 14
                bulletbox_y := pos_y+8
                bulletbox_speed := 8
                
                
            HandleBullets

        
           ' repeat x from 0 to 1000            
            
            gfx.DrawScreen
            
            
PUB HandleBullets
            if bulletbox

'                if bulletbox_speed < 
 '                   bulletbox_speed += 1
               
                if bulletbox_dir == 0
                    bulletbox_y -= bulletbox_speed                
                if bulletbox_dir == 1
                    bulletbox_x += bulletbox_speed                
                if bulletbox_dir == 2
                    bulletbox_y += bulletbox_speed                
                if bulletbox_dir == 3
                    bulletbox_x -= bulletbox_speed                                    

                if bulletbox_x - xoffset =< 0 or bulletbox_x - xoffset => SCREEN_W or bulletbox_y - yoffset =< 0 or bulletbox_y - yoffset => SCREEN_H
                    bulletbox := 0
                else
                    gfx.Sprite(@gfx_laser,bulletbox_x - xoffset,bulletbox_y - yoffset,0)


PUB ControlOffset | bound_x, bound_y

    bound_x := byte[@map_supersidescroll][0]<<3 - SCREEN_W
    bound_y := byte[@map_supersidescroll][1]<<3 - SCREEN_H
    
    xoffset := pos_x + (PLAYER_W>>1) - (SCREEN_W>>1)
    if xoffset < 0
        xoffset := 0      
    elseif xoffset > bound_x
        xoffset := bound_x
        
        
    yoffset := pos_y + (PLAYER_H>>1) - (SCREEN_H>>1)
    if yoffset < 0
        yoffset := 0      
    elseif yoffset > bound_y
        yoffset := bound_y


